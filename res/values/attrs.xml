<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="mulTabActionBar">
        <!-- switchTitle 高度 -->
        <attr name="mtabTabHeight" format="dimension" />
        <!-- 整个背景色 -->
        <attr name="mtabBackground" format="reference|color" />
        <!-- ActionBar style -->
        <attr name="mActionBarStyle" format="reference" />
    </declare-styleable>
    
    <declare-styleable name="switchTitle">
        <attr name="tabTxtActiveColor" format="color" />
        <attr name="tabTxtDisableColor" format="color" />
        <attr name="tabLineColor" format="color" />
        <attr name="tabTxtSize" format="dimension" />
        <attr name="tabLineHeight" format="dimension" />
        <attr name="tabPadding" format="dimension" />
    </declare-styleable>

</resources>